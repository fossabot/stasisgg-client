{
  "name": "stasisgg-client",
  "productName": "stasisgg-client",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "dev": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "test": "jest --verbose"
  },
  "keywords": [],
  "author": {
    "name": "R-NK",
    "email": "right.rcon.f@outlook.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "stasisgg_client"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.tsx",
                  "name": "main_window"
                }
              ]
            },
            "loggerPort": 9002
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.9.0",
    "@material-ui/core": "^4.4.3",
    "@material-ui/icons": "^4.4.3",
    "@use-it/interval": "^0.1.3",
    "axios": "^0.19.0",
    "electron-squirrel-startup": "^1.0.0",
    "fp-ts": "^2.1.0",
    "io-ts": "^2.0.1",
    "lowdb": "^1.0.0",
    "notistack": "^0.9.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-hot-loader": "^4.12.12",
    "react-router-dom": "^5.0.1",
    "react-sidebar": "^3.0.2",
    "react-spinners": "^0.6.1",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.45",
    "@electron-forge/maker-deb": "6.0.0-beta.45",
    "@electron-forge/maker-rpm": "6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "6.0.0-beta.45",
    "@electron-forge/maker-zip": "6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "6.0.0-beta.45",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@types/electron-load-devtool": "^1.0.1",
    "@types/lowdb": "^1.0.9",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/react-sidebar": "^3.0.0",
    "@types/styled-components": "^4.1.19",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "css-loader": "^3.0.0",
    "electron": "6.0.8",
    "electron-load-devtool": "^1.2.0",
    "eslint": "^6.3.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^4.2.0",
    "jest": "^24.9.0",
    "node-loader": "^0.6.0",
    "prettier": "^1.18.2",
    "react-svg-loader": "^3.0.3",
    "style-loader": "^0.23.1",
    "styled-components": "^5.0.0-beta.8",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.1.0",
    "typescript": "^3.6.3",
    "typescript-plugin-styled-components": "^1.4.3",
    "webpack-cli": "^3.3.8"
  }
}
